  <fhirVersion value="1.0.2" />
  <kind value="resource" />
  <constrainedType value="Condition" />
  <abstract value="false" />
  <base value="http://hl7.org/fhir/StructureDefinition/Condition" />
  <differential>
    <element>
      <path value="Condition" />
      <short value="Detailed information about problems" />
      <definition value="Use to record detailed information about problems recognised by a clinician. " />
      <type>
        <code value="Condition" />
      </type>
    </element>
    <element>
      <path value="Condition.extension" />
      <slicing>
        <discriminator value="url" />
        <rules value="openAtEnd" />
      </slicing>
    </element>
    <element>
      <path value="Condition.extension" />
      <name value="related" />
      <short value="Related problem - combined | grouped | replaces | evolved-from" />
      <definition value="A reference to another problem (target) whose relationship is defined by the relationship type code." />
      <min value="0" />
      <max value="*" />
      <type>
        <code value="Extension" />
        <profile value="http://hl7.org.uk/CareConnect-ProblemRelationship-1-Extension.structuredefinition.xml" />
      </type>
    </element>
    <element>
      <path value="Condition.extension" />
      <name value="associated" />
      <short value="Associated resource" />
      <definition value="A reference to another resource (e.g. MedicationOrder, Observation, ReferralRequest etc.) associated/linked to this condition." />
      <min value="0" />
      <max value="*" />
      <type>
        <code value="Extension" />
        <profile value="http://hl7.org.uk/CareConnect-ProblemAssociated-1-Extension.structuredefinition.xml" />
      </type>
    </element>
    <element>
      <path value="Condition.identifier.use" />
      <max value="0" />
    </element>
    <element>
      <path value="Condition.identifier.type" />
      <max value="0" />
    </element>
    <element>
      <path value="Condition.identifier.value" />
      <min value="1" />
      <exampleString value="123456" />
    </element>
    <element>
      <path value="Condition.identifier.period" />
      <max value="0" />
    </element>
    <element>
      <path value="Condition.identifier.assigner" />
      <max value="0" />
    </element>
    <element>
      <path value="Condition.patient" />
      <type>
        <code value="Reference" />
        <profile value="http://hl7.org.uk/CareConnect-Patient-1.structuredefinition.xml" />
      </type>
    </element>
    <element>
      <path value="Condition.encounter" />
      <type>
        <code value="Reference" />
        <profile value="http://hl7.org.uk/CareConnect-Encounter-1.structuredefinition.xml" />
      </type>
    </element>
    <element>
      <path value="Condition.asserter" />
      <short value="Author" />
      <definition value="Individual who is authoring the problem statement." />
      <min value="1" />
      <type>
        <code value="Reference" />
        <profile value="http://hl7.org.uk/CareConnect-Patient-1.structuredefinition.xml" />
      </type>
      <type>
        <code value="Reference" />
        <profile value="http://hl7.org.uk/CareConnect-Practitioner-1.structuredefinition.xml" />
      </type>
    </element>
    <element>
      <path value="Condition.dateRecorded" />
      <short value="Date entered" />
      <definition value="The date when the problem statement was documented." />
      <min value="1" />
    </element>
    <element>
      <path value="Condition.code" />
      <short value="Identification of the problem" />
      <definition value="Identification of the problem." />
      <binding>
        <strength value="example" />
        <valueSetReference>
          <reference value="http://hl7.org/fhir/ValueSet/condition-code" />
        </valueSetReference>
      </binding>
    </element>
    <element>
      <path value="Condition.code.coding" />
      <slicing>
        <discriminator value="system" />
        <ordered value="true" />
        <rules value="openAtEnd" />
      </slicing>
      <min value="1" />
    </element>
    <element>
      <path value="Condition.code.coding.extension" />
      <slicing>
        <discriminator value="url" />
        <rules value="openAtEnd" />
      </slicing>
    </element>
    <element>
      <path value="Condition.code.coding.extension" />
      <name value="qualifier" />
      <short value="Coding Qualifier" />
      <definition value="Used to qualify or indicate extra information about a coded value e.g. Laterality, Severity, etc." />
      <min value="0" />
      <max value="*" />
      <type>
        <code value="Extension" />
        <profile value="http://hl7.org.uk/CareConnect-CodingQualifier-1-Extension.structuredefinition.xml" />
      </type>
    </element>
    <element>
      <path value="Condition.code.coding" />
      <name value="SNOMED CT" />
      <short value="SNOMED CT representation identifying the problem" />
      <max value="1" />
      <binding>
        <strength value="preferred" />
        <description value="Identification of the problem" />
      </binding>
    </element>
    <element>
      <path value="Condition.code.coding.extension" />
      <slicing>
        <discriminator value="url" />
        <rules value="openAtEnd" />
      </slicing>
    </element>
    <element>
      <path value="Condition.code.coding.extension" />
      <name value="qualifier" />
      <short value="Coding Qualifier" />
      <definition value="Used to qualify or indicate extra information about a coded value e.g. Laterality, Problem Significance etc." />
      <min value="0" />
      <max value="*" />
      <type>
        <code value="Extension" />
        <profile value="http://hl7.org.uk/CareConnect-CodingQualifier-1-Extension.structuredefinition.xml" />
      </type>
    </element>
    <element>
      <path value="Condition.code.coding.system" />
      <min value="1" />
      <fixedUri value="http://snomed.info/sct" />
    </element>
    <element>
      <path value="Condition.code.coding.version" />
      <max value="0" />
    </element>
    <element>
      <path value="Condition.code.coding.code" />
      <min value="1" />
    </element>
    <element>
      <path value="Condition.code.coding.display" />
      <min value="1" />
    </element>
    <element>
      <path value="Condition.category" />
      <short value="Coded category indicating a problem condition" />
      <definition value="A coded category assigned to the condition resource to indicate the representation of a problem." />
      <min value="1" />
      <binding>
        <strength value="required" />
        <description value="A category assigned to the condition" />
        <valueSetReference>
          <reference value="http://hl7.org.uk/CareConnect-ProblemConditionCategory-1.valueset" />
        </valueSetReference>
      </binding>
    </element>
    <element>
      <path value="Condition.category.coding" />
      <short value="Problem category code" />
      <definition value="Coded identification of a Problem" />
      <min value="1" />
      <max value="1" />
    </element>
    <element>
      <path value="Condition.category.coding.system" />
      <min value="1" />
    </element>
    <element>
      <path value="Condition.category.coding.version" />
      <max value="0" />
    </element>
    <element>
      <path value="Condition.category.coding.code" />
      <min value="1" />
      <fixedCode value="problem" />
    </element>
    <element>
      <path value="Condition.category.coding.display" />
      <fixedString value="Problem" />
    </element>
    <element>
      <path value="Condition.category.coding.userSelected" />
      <max value="0" />
    </element>
    <element>
      <path value="Condition.category.text" />
      <max value="0" />
    </element>
    <element>
      <path value="Condition.clinicalStatus" />
      <binding>
        <strength value="required" />
        <description value="The clinical status of the problem." />
        <valueSetReference>
          <reference value="http://hl7.org/fhir/ValueSet/condition-clinical" />
        </valueSetReference>
      </binding>
    </element>
    <element>
      <path value="Condition.verificationStatus" />
      <binding>
        <strength value="required" />
        <description value="The verification status to support or decline the clinical status of the problem." />
        <valueSetReference>
          <reference value="http://hl7.org/fhir/ValueSet/condition-ver-status" />
        </valueSetReference>
      </binding>
    </element>
    <element>
      <path value="Condition.severity.coding.version" />
      <max value="0" />
    </element>
    <element>
      <path value="Condition.onsetDateTime" />
      <short value="Estimated or actual date or date-time" />
      <definition value="Estimated or actual date or date-time the problem began, in the opinion of the clinician." />
      <type>
        <code value="dateTime" />
      </type>
    </element>
    <element>
      <path value="Condition.abatementDateTime" />
      <short value="When in resolution/remission" />
      <definition value="The date or estimated date that the problem resolved or went into remission. This is called &quot;abatement&quot; because of the many overloaded connotations associated with &quot;remission&quot; or &quot;resolution&quot; - Conditions are never really resolved, but they can abate." />
      <type>
        <code value="dateTime" />
      </type>
    </element>
    <element>
      <path value="Condition.stage" />
      <max value="0" />
    </element>
    <element>
      <path value="Condition.evidence" />
      <max value="0" />
    </element>
    <element>
      <path value="Condition.bodySite" />
      <max value="0" />
    </element>
    <element>
      <path value="Condition.notes" />
      <short value="Additional information about the problem" />
      <definition value="Additional information about the problem. This is a general notes/comments entry for description of the Condition, its diagnosis and prognosis." />
    </element>
  </differential>
</StructureDefinition>
