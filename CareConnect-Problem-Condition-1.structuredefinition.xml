<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <meta>
  </meta>
  <url value="http://hl7.org.uk/CareConnect-Problem-Condition-1.structuredefinition.xml" />
  <version value="1.0" />
  <name value="CareConnect-Problem-Condition-1" />
  <status value="draft" />
  <publisher value="HL7 UK" />
  <description value="Problem Condition resource is used to record detailed information about problems recognised by a clinician." />
