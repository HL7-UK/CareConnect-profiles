	<url value="http://hl7.org.uk/CareConnect-ProblemRelationshipType-1.valueset.xml"/>
	<version value="1.0.0"/>
	<name value="CareConnect-ProblemRelationshipType-1"/>
	<status value="draft"/>
	<publisher value="HL7 UK"/>
	<description value="A set of codes specifying the kind of relationship that exists with a target problem."/>
	<codeSystem>
		<system value="http://hl7.org.uk/fhir/ValueSet/CareConnect-ProblemRelationshipType"/>
		<concept>
			<code value="combined"/>
			<display value="Combined"/>
			<definition value="This problem is combined/merged with the target problem to form a single logical problem."/>
		</concept>
		<concept>
			<code value="grouped"/>
			<display value="Grouped"/>
			<definition value="This problem is part of a group that includes the target problem as a member of the group."/>
		</concept>
		<concept>
			<code value="replaces"/>
			<display value="Replaces"/>
			<definition value="This problem replaces a previous problem (i.e. a revised diagnosis). The target problem is now obsolete."/>
		</concept>
		<concept>
			<code value="evolved-from"/>
			<display value="Evolved From"/>
			<definition value="This problem has evolved from the target problem"/>
		</concept>
	</codeSystem>
</ValueSet>
